<main role="main" class="inner cover">
<h1 class="cover-heading">Tech News</h1>
  <div class="row">
        <% @tweets.each do |tweet| %>
          <div class="col-md-4">
            <div class="tech-news rounded border border-primary">
              <%= twitter_parser tweet %>
            </div>
          </div>
        <% end %>
  </div>
</main>

